{
    "configurations": [
        {
            "name": "workspace",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "compilerPath": "/usr/local/bin/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/app/**",
                "${workspaceFolder}/backbone/**",
                "${workspaceFolder}/config/**",
                "${workspaceFolder}/mcu/**",
                "${workspaceFolder}/modifiers/**",
                "${workspaceFolder}/port/**",
                "${workspaceFolder}/ui/**",
                "${workspaceFolder}/voices/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/app",
                    "${workspaceFolder}/backbone",
                    "${workspaceFolder}/config",
                    "${workspaceFolder}/mcu",
                    "${workspaceFolder}/modifiers",
                    "${workspaceFolder}/port",
                    "${workspaceFolder}/ui",
                    "${workspaceFolder}/voices"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [
                "STM32F401xC",
                "USE_FULL_LL_DRIVER"
            ]
        }
    ],
    "version": 4
}